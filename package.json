{
  "name": "image_generator",
  "version": "1.0.0",
  "main": "src/server.ts",
  "license": "MIT",
  "scripts": {
    "start": "nodemon"
  },
  "nodemon": {
    "signal": "SIGTERM",
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "ts,json",
    "watch": "src",
    "exec": "ts-node -- src/server.ts"
  },
  "dependencies": {
    "express": "^4.18.1",
    "playwright": "^1.24.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.2",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
